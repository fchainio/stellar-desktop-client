{
  "comments": false,
  "env": {
    "main": {
      "presets": [
        ["env", {
          "targets": { "node": 7 }
        }],
        "es2015",
        "stage-0"
      ]
    },
    "renderer": {
      "presets": [
        ["env", {
          "modules": false
        }],
        "es2015",
        "stage-0"
      ]
    },
    "web": {
      "presets": [
        ["env", {
          "modules": false
        }],
        "es2015",
        "stage-0"
      ]
    }
  },
  "plugins": [
    "transform-runtime",
    "add-filehash",
    "equire"
  ]
}
